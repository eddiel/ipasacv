<%- model_class = @duty.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:fecha) %>:</strong></dt>
  <dd><%= @duty.fecha %></dd>
  <dt><strong><%= model_class.human_attribute_name(:monto) %>:</strong></dt>
  <dd><%= @duty.monto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:property_id) %>:</strong></dt>
  <dd><%= @duty.property_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              duties_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_duty_path(@duty), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              duty_path(@duty),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
